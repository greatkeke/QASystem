  </configSections>
  <connectionStrings>
    <add name="DefaultConnection" connectionString="Data Source=(LocalDb)\MSSQLLocalDB;AttachDbFilename=|DataDirectory|\aspnet-WebApplication1-20160312054514.mdf;Initial Catalog=aspnet-WebApplication1-20160312054514;Integrated Security=True" providerName="System.Data.SqlClient" />
